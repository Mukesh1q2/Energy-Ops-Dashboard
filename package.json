{
  "name": "nextjs_tailwind_shadcn_ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"npx tsx server.ts\" --watch server.ts --watch src --ext ts,tsx,js,jsx",
    "dev:safe": "npm run kill-port && npm run dev",
    "dev:clean": "npm run db:push:safe && npm run dev:safe",
    "kill-port": "taskkill /f /im node.exe 2>nul || echo \"No Node processes to kill\"",
    "build": "next build",
    "start": "tsx server.ts",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:push": "prisma db push",
    "db:push:safe": "npm run kill-port && prisma db push",
    "db:generate": "prisma generate",
    "db:generate:safe": "npm run kill-port && prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:safe": "npm run kill-port && prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "cleanup:uploads": "tsx scripts/cleanup-uploads.ts",
    "cleanup:uploads:dry": "tsx scripts/cleanup-uploads.ts --dry-run"
  },
